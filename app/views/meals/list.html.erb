<div class="container">
  <div class="row">
    <h3>Meals</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @meals.each do |meal| %>
            <tr>
              <%= render meal %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
